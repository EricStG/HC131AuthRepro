    <ItemGroup>
      <PackageReference Include="HotChocolate.AspNetCore" Version="$(HotChocolateVersion)" />
      <PackageReference Include="HotChocolate.AspNetCore.Authorization" Version="$(HotChocolateVersion)" />
    </ItemGroup>

</Project>
